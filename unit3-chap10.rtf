\par \pard\plain \s18\loch\sl276\slmult1\sb0\sa140\loch{\listtext\pard\plain \rtlch\af5 \ltrch\hich\af5\loch\f5\dbch\af5 \u8226\'95\tab}\ilvl0\ls31 \li0\ri0\lin0\rin0\fi-283\ql\tx707\li707\ri0\lin707\rin0\fi-283\box\brdrhair\brdrw1\brdrcf17\brsp28\ltrpar{\loch\chbrdr\brdrhair\brdrw1\brdrcf17\brsp28\loch
